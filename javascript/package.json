{
    "name": "qianfan-sdk",
    "version": "0.0.2",
    "type": "commonjs",
    "main": "dist/qianfan-sdk.umd.js",
    "module": "dist/qianfan-sdk.es.js",
    "files": [
        "dist/*"
    ],
    "scripts": {
        "dev": "vite",
        "build": "tsc && vite build --mode npm",
        "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
        "preview": "vite preview",
        "test": "vitest",
        "coverage": "vitest run --coverage",
        "publish-main": "npm run build && npm publish --access public",
        "publish-beta": "npm run publish-main --tag beta"
    },
    "dependencies": {
        "axios": "^1.6.6",
        "react": "^18.2.0",
        "react-dom": "^18.2.0"
    },
    "devDependencies": {
        "@babel/eslint-parser": "^7.23.3",
        "@babel/eslint-plugin": "^7.23.5",
        "@ecomfe/eslint-config": "^8.0.0",
        "@types/react": "^18.2.43",
        "@types/react-dom": "^18.2.17",
        "@typescript-eslint/eslint-plugin": "^6.14.0",
        "@typescript-eslint/parser": "^6.14.0",
        "@vitejs/plugin-react": "^4.2.1",
        "eslint": "^8.56.0",
        "eslint-plugin-react": "^7.33.2",
        "eslint-plugin-react-hooks": "^4.6.0",
        "eslint-plugin-react-refresh": "^0.4.5",
        "typescript": "^5.2.2",
        "vite": "^5.0.8",
        "vitest": "^1.2.1"
    }
}